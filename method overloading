using System;
using System.Collections.Generic;
using System.Linq;
using System.Security.Cryptography.X509Certificates;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp1
{
	internal class Program
	{

		static void Main(string[] args)
		{

			int num1 =6 ;
			int num2 = 5;
			int num3 = 4;
			Console.WriteLine(ahmet(num1, num2));
			Console.WriteLine(ahmet(  num1, num2,num3)); 
 

			Console.ReadLine();
		}
		static int ahmet(  int num1, int num2)
		{
			num1 = 6;
			return num1 + num2;

		}
		static int ahmet(int num1, int num2,int num3)
		{
			num1 = 6;
			return num1 + num2+num3;

		}
	}
	
}
